{
  "name": "@just4/ua-info",
  "version": "0.2.0-beta.1",
  "description": "Just for user agent analysing.",
  "keywords": ["useragent"],
  "scripts": {
    "link-deps": "ln -s ../../node_modules ./node_modules",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js",
    "lint": "eslint ./src/*.ts",
    "prebuild": "rimraf ./dist/*",
    "build": "npm run lint && tsc -p ./src/tsconfig.json && node ../../build/compress ua-info && node ../../build/copy-others ua-info",
    "gen-doc": "typedoc --tsconfig ./src/tsconfig.json"
  },
  "private": true
}
