{
  "name": "@just4/ajax",
  "version": "0.4.0-beta.2",
  "description": "Just for making XMLHTTPRequest easier to use in browser.",
  "keywords": ["ajax", "XMLHttpRequest", "XDomainRequest"],
  "scripts": {
    "link-deps": "ln -s ../../node_modules ./node_modules",
    "dev": "webpack-dev-server --config ../../build/webpack.dev.config.js",
    "lint": "eslint ./src/*.ts",
    "prebuild": "rimraf ./dist/*",
    "build": "npm run lint && tsc -p ./src/tsconfig.json && node ../../build/compress && node ../../build/copy-others",
    "gen-doc": "typedoc --options ../typedoc.js --tsconfig ./src/tsconfig.json"
  },
  "dependencies": {
    "@just4/util": "^0.2.0-beta.1",
    "@just4/querystring": "^0.2.0-beta.2"
  },
  "private": true,
  "devPort": 8606
}
