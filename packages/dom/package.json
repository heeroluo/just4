{
  "name": "@just4/dom",
  "version": "0.1.0",
  "description": "A library for dom manipulation in browser.",
  "scripts": {
    "link-deps": "ln -s ../../node_modules ./node_modules",
    "link-to-node-modules": "node ../../build/link-to-node-modules dom",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js",
    "lint": "eslint ./src/*.ts",
    "prebuild": "rimraf ./dist/*",
    "build": "npm run lint && tsc && node ../../build/copy-others dom",
    "pregen-doc": "rimraf ../../docs/dom",
    "gen-doc": "typedoc --out ../../docs/dom"
  },
  "dependencies": {
    "@just4/util": "^0.1.0"
  }
}
